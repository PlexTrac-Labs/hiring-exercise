  "main": "build/index.js",
  "dependencies": {
    "@hapi/hapi": "18.4.0",
    "axios": "^0.24.0",
    "bcryptjs": "2.4.3",
    "good": "8.1.2",
    "good-console": "8.0.0",
    "good-file": "6.0.1",
    "good-squeeze": "5.1.0",
    "hapi-auth-jwt2": "8.8.1",
    "hapi-pino": "6.3.0",
    "joi": "14.3.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.8.11"
  },
  "devDependencies": {
    "@types/hapi": "18.0.3",
    "@types/node": "13.7.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.0.9",
    "mocha": "7.0.1",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
